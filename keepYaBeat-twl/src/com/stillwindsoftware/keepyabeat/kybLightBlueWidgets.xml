        <animation name="editfield.cursor" timeSource="keyboardFocus">
            <frame ref="-editfield.cursor" duration="500"/>
            <frame ref="none" duration="500"/>
        </animation>
        <alias name="editfield.selection" ref="frameTitleColourSpot"/>

        <animation name="radiobutton.show" timeSource="selected" count="1" frozenTime="200">
            <frame duration="50" ref="radioMark" zoom="0.2"/>
            <frame duration="150" ref="radioMark" zoom="1.4"/>
            <frame duration="0" ref="radioMark"/>
        </animation>
        <animation name="radiobutton.hide" timeSource="selected" count="1" frozenTime="200">
            <frame ref="radioMark" duration="200" zoomCenterY="0.75"/>
            <frame ref="radioMark" duration="0" zoom="0" zoomCenterY="0.75"/>
        </animation>
        <grid name="radiobutton.background" weightsX="0,1" weightsY="1" border="1,25,1,1">
            <select center="true">
                <composed if="selected + disabled">
                    <alias ref="radioBackgroundDisabled"/>
                    <alias ref="radioMarkDisabled" center="true"/>
                </composed>
                <alias ref="radioBackgroundDisabled" if="disabled"/>
                <composed if="selected + armed">
                    <alias ref="radioBackground"/>
                    <alias ref="radiobutton.show" center="true"/>
                </composed>
                <alias ref="radiobutton.show" if="armed"/>
                <composed if="selected">
                    <alias ref="radioBackground"/>
                    <alias ref="radiobutton.show" center="true"/>
                </composed>
                <alias ref="radioBackground"/>
            </select>
            <alias ref="none"/>
        </grid>

        <select name="checkbox.background">
            <alias ref="checkboxBackgroundDisabled" if="disabled"/>
            <alias ref="checkboxBackground"/>
        </select>
        <animation name="checkbox.show" timeSource="selected" count="1" frozenTime="200">
            <frame duration="50" ref="checkMark" zoom="0.2"/>
            <frame duration="150" ref="checkMark" zoom="1.4"/>
            <frame duration="0" ref="checkMark"/>
        </animation>
        <animation name="checkbox.hide" timeSource="selected" count="1" frozenTime="200">
            <frame ref="checkMark" duration="200" zoomCenterY="0.75"/>
            <frame ref="checkMark" duration="0" zoom="0" zoomCenterY="0.75"/>
        </animation>
        <select name="checkbox.overlay">
            <alias ref="checkMarkDisabled" if="selected + disabled"/>
            <alias ref="checkbox.show" if="selected"/>
            <alias ref="checkbox.hide"/>
        </select>

